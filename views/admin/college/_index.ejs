<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <link rel="stylesheet" href="../../static/app/css/admincss/style.css" />
    <title>admin dashboard</title>
</head>

<body>
    <div class="header">
        <div class="top">
            <div class="logo">
                <h2>My Business</h2>
            </div>
            <div class="close">
                <span class="material-symbols-outlined operateC hide" onclick="closeMenuBar(this)">
                    close
                </span>
                <span class="material-symbols-outlined operateM" onclick="menuBar(this)">
                    menu
                </span>
            </div>
        </div>
    </div>
    <div class="container">
        <aside class="">
            <!-- sidebar -->
            <div class="sidebar">
                <a href="#" class="active"><span class="material-symbols-outlined">dashboard</span>
                    <h3>dashboard</h3>
                </a>
                <a href="#"><span class="material-symbols-outlined">
                        person_add
                    </span>
                    <h3>add student</h3>
                </a>
                <a href="/coll-gallery"><span class="material-symbols-outlined">
                        fact_check
                    </span>
                    <h3>gallery</h3>
                </a>
                <a href="#"><span class="material-symbols-outlined">
                        groups
                    </span>
                    <h3>stuff</h3>
                </a>
                <a href="#"><span class="material-symbols-outlined">
                        logout
                    </span>
                    <h3>logout</h3>
                </a>
            </div>
        </aside>
        <!-- main section -->
        <main>
            <div class="searchBar">
                <input type="text" placeholder="Search by name">
                <span class="material-symbols-outlined">
                    search
                </span>
            </div>
            <div class="recent-adm">
                <h2>
                    College Admission
                </h2>

                <table>
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Ph. no</th>
                        <th>Class</th>
                    </tr>
                    <% result?.forEach((data)=>{ %>
                        <tr>
                            <td><%= data.id %></td>
                            <td><%= data.name %></td>
                            <td><%= data.phno %></td>
                            <td class="td"><%= data.stream %> <span class="material-symbols-outlined" data-stid="<%= data.id %>" onclick="getCollegeEachStudentData(this)">unfold_more</span></td>
                        </tr>
                    <% }) %>
                </table>
            </div>
        </main>
    </div>

    <div class="overlay hide"></div>
    <div class="pop-content hide">
        <button id="close-pop" class="drop-title btn" onclick="closePopup()"><span class="material-symbols-outlined">
            close
        </span></button>
        <div class="form-el">
           
        </div>
    </div>

    <script src="../../static/app/js/script.js"></script>
</body>

</html>